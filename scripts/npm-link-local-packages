#!/usr/bin/env bash

PACKAGES=(
  "ai-grep"
  "ai-translate"
  "alfred-infinite-clipboard"
  "archivist/archivist-cli"
  "logger-search"
  "muninn-get-asset"
  "muninn-make-month"
  "muninn-simple"
  "timav-cli"
  "timav-dashboard"
)

for dir in "${PACKAGES[@]}"; do
  pushd "$HOME/Documents/Projects/$dir" || exit
  npm install
  npm link
  popd || exit
done
