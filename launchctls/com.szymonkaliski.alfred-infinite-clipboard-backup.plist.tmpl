<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC -//Apple Computer//DTD PLIST 1.0//EN http://www.apple.com/DTDs/PropertyList-1.0.dtd>
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.szymonkaliski.alfred-infinite-clipboard-backup</string>
        <key>ProgramArguments</key>
        <array>
            <string>/bin/bash</string>
            <string>LAUNCHCTL_SCRIPTS_PATH/alfred-infinite-clipboard-backup.sh</string>
        </array>
        <key>RunAtLoad</key>
        <true/>
        <key>StartInterval</key>
        <integer>86400</integer>
        <key>StandardErrorPath</key>
        <string>/tmp/alfred-infinite-clipboard-backup.err</string>
        <key>StandardOutPath</key>
        <string>/tmp/alfred-infinite-clipboard-backup.out</string>
    </dict>
</plist>
